{
  "name": "vue-virtual-picklist",
  "version": "0.0.2",
  "description": "This is an optimized picklist/combobox for Vue that virtualizes the available options",
  "author": {
    "name": "Allan Oricil",
    "email": "allanoricilcos@outlook.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AllanOricil/vue-virtual-picklist.git"
  },
  "bugs": {
    "url" : "https://github.com/AllanOricil/vue-virtual-picklist/issues",
    "email" : "allanoricilcos@outlook.com"
  },
  "license": "Apache-2.0",
  "funding": "https://www.buymeacoffee.com/allanoricil",
  "keywords": ["vue", "picklist", "virtual", "virtualized", "combobox", "nuxt", "optimized"],
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "test:unit": "vue-cli-service test:unit",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "format": "prettier .  --write",
    "prebuild": "rimraf ./dist"
  },
  "main": "dist/vue-virtual-picklist.ssr.js",
  "module": "dist/vue-virtual-picklist.esm.js",
  "browser": "dist/vue-virtual-picklist.esm.js",
  "unpkg": "dist/vue-virtual-picklist.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "dependencies": {
    "v-click-outside": "^3.1.2",
    "vue-virtualized-list": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@rollup/plugin-alias": "^3.1.2",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "@vue/cli-plugin-babel": "^4.5.13",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "^4.5.13",
    "@vue/test-utils": "^1.2.2",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^7.18.0",
    "minimist": "^1.2.5",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.52.8",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "sinon": "^11.1.2",
    "vue": "^2.6.14",
    "vue-template-compiler": "^2.6.14"
  },
  "peerDependencies": {
    "vue": "^2.6.14"
  },
  "engines": {
    "node": ">=12"
  },
  "sideEffects": false
}
